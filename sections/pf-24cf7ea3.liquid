    </script><script>document.addEventListener('DOMContentLoaded', function () {
  /* START Show custom subscrition prod widget */
  const button = document.querySelector('.add-seeds-button');
  const widget = document.querySelector('.subscription-widget');

  if (!button || !widget) return;

  // --- slide helpers ---
  function slideUp(el, duration = 300) {
    el.style.overflow = 'hidden';
    el.style.maxHeight = el.scrollHeight + 'px';
    el.offsetHeight; // force reflow
    el.style.transition = `max-height ${duration}ms ease`;
    el.style.maxHeight = '0px';
    window.setTimeout(() => {
      el.style.display = 'none';
      el.style.removeProperty('max-height');
      el.style.removeProperty('overflow');
      el.style.removeProperty('transition');
    }, duration);
  }

  function slideDown(el, duration = 300, displayType = 'block') {
    el.style.removeProperty('display');
    if (getComputedStyle(el).display === 'none') el.style.display = displayType;
    const height = el.scrollHeight;
    el.style.overflow = 'hidden';
    el.style.maxHeight = '0px';
    el.offsetHeight; // force reflow
    el.style.transition = `max-height ${duration}ms ease`;
    el.style.maxHeight = height + 'px';
    window.setTimeout(() => {
      el.style.removeProperty('max-height');
      el.style.removeProperty('overflow');
      el.style.removeProperty('transition');
    }, duration);
  }

  function slideToggle(el, duration = 300) {
    if (getComputedStyle(el).display === 'none') {
      slideDown(el, duration);
    } else {
      slideUp(el, duration);
    }
  }
  // --- /slide helpers ---

  // Start hidden by default
  widget.style.display = 'none';

  const addIcon = button.querySelector('.add-icon');
  const addedIcon = button.querySelector('.added-icon');
  const addText = button.querySelector('.add-text');

  button.addEventListener('click', function () {
    const isClicked = button.classList.contains('clicked');

    if (isClicked) {
      // Revert
      button.classList.remove('clicked');
      slideUp(widget, 250);
      if (addIcon) addIcon.style.display = '';
      if (addedIcon) addedIcon.style.display = 'none';
      if (addText) addText.textContent = 'Add';
    } else {
      // Activate
      button.classList.add('clicked');
      slideDown(widget, 250);
      if (addIcon) addIcon.style.display = 'none';
      if (addedIcon) addedIcon.style.display = 'block';
      if (addText) addText.textContent = 'Added';
    }
  });
  /* END Show custom subscrition prod widget */

  /* START Adding label to color swatches */
  const swatchesContainer = document.querySelector('.pf-option-swatches');
  if (!swatchesContainer) return;

  const swatchDivs = swatchesContainer.querySelectorAll('div');
  if (swatchDivs.length < 2) return;

  // Create <p> for Orange
  const orangeP = document.createElement('p');
  orangeP.textContent = 'Orange';
  orangeP.style.marginLeft = '5px';

  // Create <p> for Beige
  const beigeP = document.createElement('p');
  beigeP.textContent = 'Beige';
  beigeP.style.marginLeft = '5px';

  swatchDivs[0].appendChild(orangeP);
  swatchDivs[1].appendChild(beigeP);
  /* END Adding label to color swatches */

  /* START custom select icon */
  const WRAPPER_SELECTOR = '.svg-wrapper'; // outer container for the group
  const OPTION_SELECTOR = '.seed-option';  // each clickable option
  const ORANGE = '#FF7A00';

  function createInnerCircle(svg) {
    const vb = (svg.getAttribute('viewBox') || '0 0 512 512').split(/\s+/).map(Number);
    const [x, y, w, h] = vb.length === 4 ? vb : [0, 0, 512, 512];
    const cx = x + w / 2;
    const cy = y + h / 2;
    const r = Math.max(2, Math.min(w, h) / 2 - 36);

    const c = document.createElementNS('http://www.w3.org/2000/svg', 'circle');
    c.setAttribute('class', 'inner-white-circle');
    c.setAttribute('cx', cx);
    c.setAttribute('cy', cy);
    c.setAttribute('r', r);
    c.setAttribute('fill', '#ffffff');
    c.setAttribute('stroke', ORANGE);
    c.setAttribute('stroke-width', '2');
    c.setAttribute('vector-effect', 'non-scaling-stroke');
    return c;
  }

  function applyState(groupWrapper) {
    const options = groupWrapper.querySelectorAll(OPTION_SELECTOR);
    options.forEach(option => {
      const svg = option.querySelector('svg');
      const circle = svg.querySelector('.inner-white-circle');
      if (option.classList.contains('selected')) {
        if (circle) circle.remove();
        svg.querySelectorAll('path').forEach(p => p.setAttribute('fill', ORANGE));
      } else {
        if (!circle) svg.appendChild(createInnerCircle(svg));
        svg.querySelectorAll('path').forEach(p => p.removeAttribute('fill'));
      }
    });
  }

  function selectOption(groupWrapper, targetOption) {
    groupWrapper.querySelectorAll(OPTION_SELECTOR).forEach(opt => opt.classList.remove('selected'));
    targetOption.classList.add('selected');
    applyState(groupWrapper);
  }

  // Init
  document.querySelectorAll(WRAPPER_SELECTOR).forEach(groupWrapper => {
    const options = groupWrapper.querySelectorAll(OPTION_SELECTOR);
    if (options.length === 0) return;

    if (![...options].some(opt => opt.classList.contains('selected'))) {
      options[0].classList.add('selected');
    }

    applyState(groupWrapper);

    // Make entire option clickable
    options.forEach(option => {
      option.style.cursor = 'pointer';
      option.addEventListener('click', () => {
        if (!option.classList.contains('selected')) {
          selectOption(groupWrapper, option);
        }
      });
    });
  });
  /* END custom select icon */

  /* START Cusotm code to add additinal product on cart manually */
  (function () {
    // ====== CONFIG (adjust as needed) ======
    const CONFIG = {
      // Selectors (as you provided)
      pageflyFormSelector: '.pf-product-form',
      addToCartBtnSelector: '.pf-product-form [data-pf-type="ProductATC2"]',
      addOnToggleBtnSelector: '.pf-product-form .add-seeds-button',
      addOnWidgetSelector: '.subscription-widget',
      planOptionSelector: '.seed-option',

      // Add-on product details
      addOnVariantId: 51153862066495, // <-- REQUIRED: set your add-on VARIANT ID (number)

      // After adding items
      afterAdd: 'checkout', // 'cart' | 'checkout' | 'stay'
    };

    // ====== HELPERS ======
    function serializeFormToItem(formEl) {
      const fd = new FormData(formEl);
      const id = Number(fd.get('id'));
      const quantity = Number(fd.get('quantity') || 1);
      if (!id || isNaN(id)) return null;
      return { id, quantity };
    }

    async function addItems(items) {
      const res = await fetch('/cart/add.js', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json', 'Accept': 'application/json' },
        body: JSON.stringify({ items })
      });
      if (!res.ok) {
        const err = await res.json().catch(() => ({}));
        throw new Error(err?.description || 'Failed to add to cart.');
      }
      return res.json();
    }

    function goNext() {
      switch (CONFIG.afterAdd) {
        case 'checkout':
          window.location.href = '/checkout';
          break;
        case 'cart':
          window.location.href = '/cart';
          break;
        case 'stay':
        default:
          // Try to open Impact's cart drawer (best-effort, safe to keep)
          document.querySelector('[data-cart-toggle], button[aria-controls="CartDrawer"], .header__icon--cart a')?.click();
          break;
      }
    }

    function isAddOnEnabled() {
      // Add the add-on whenever a seed option is selected
      return !!document.querySelector('.add-seeds-button.clicked');
    }

    function getSelectedSellingPlan() {
      // We rely on your code to add/remove `.selected` on the chosen plan option
      // If the selected option represents a subscription, add data-selling-plan="<id>" on that .seed-option
      const selected = document.querySelector(`${CONFIG.planOptionSelector}.selected`);
      if (!selected) return null;

      // Support both data-selling-plan and data-selling_plan attributes
      const sp = selected.dataset.sellingPlan || selected.dataset.selling_plan;
      return sp ? Number(sp) : null; // null => one-time purchase
    }

    // ====== MAIN: intercept PageFly Add to Cart ======
    const form = document.querySelector(CONFIG.pageflyFormSelector);
    const atcBtn = document.querySelector(CONFIG.addToCartBtnSelector);

    if (!form || !atcBtn) {
      console.warn('[rf] Missing PageFly form or ATC button. Check selectors in CONFIG.');
      return;
    }

    atcBtn.addEventListener('click', async (e) => {
      // Prevent PageFly's default submission so we can add multi-items ourselves
      e.preventDefault();
      e.stopPropagation();

      try {
        // Base product from the PageFly form
        const baseItem = serializeFormToItem(form);
        if (!baseItem) throw new Error('Missing base product variant id in form.');

        const items = [baseItem];

        // Optional add-on
        if (CONFIG.addOnVariantId) {
          const enabled = isAddOnEnabled();
          if (enabled) {
            const sellingPlan = getSelectedSellingPlan(); // null => one-time
            const addOn = {
              id: Number(CONFIG.addOnVariantId),
              quantity: 1,
            };
            if (sellingPlan) addOn.selling_plan = sellingPlan;
            items.push(addOn);
          }
        }

        // One multi-item add keeps cart consistent
        await addItems(items);

        // Next step
        goNext();
      } catch (err) {
        console.error('[rf] Add to cart failed:', err);
        console.log(err?.message || 'Could not add to cart. Please try again.');
      }
    });
  })();
  /* END Cusotm code to add additional product on cart manually */

});</script>
